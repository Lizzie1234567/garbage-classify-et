page{
  height: 100%;
  width: 100%;
}
.container {
  padding: 0;
  justify-content: flex-start;
  background: #dedede;
  width: 100%;
  height: 100%;
}
.scroll-view{
  height: 100%;
  
}
.root{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.search_box{
  background: #ffffff;
  display: flex;
  justify-content: center;
  padding: 20rpx 0 20rpx 0;
  width: 100%;
}
.search_inp {
  width: 90%;
  text-align: center;
  font-size: 25rpx;
  padding: 5rpx 15rpx;
  background: #dedede;
  border-radius: 15rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.list{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  padding-bottom: 30rpx;
  font-size: 25rpx;
}
.list .item{
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin: 10rpx 0 10rpx 0;
  background: #fff;
  border-radius: 15rpx;
  overflow: hidden;
  padding: 20rpx;
}