page{
  height: 100%;
  width: 100%;
}
.container {
  padding: 0;
  justify-content: flex-start;
  background: #dedede;
  width: 100%;
  height: 100%;
}
.scroll-view{
  height: 100%;
  
}
.root{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 15rpx 30rpx 15rpx 30rpx;
  padding: 20rpx;
}
.item{
  padding: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
  width: 90%;
}
.item .icon {
  width: 45rpx;
  height: 30rpx;
}