page{
  height: 100%;
  width: 100%;
}
.container {
  padding: 0;
  justify-content: flex-start;
  background: #dedede;
  width: 100%;
  height: 100%;
}
.scroll-view{
  background: #ffffff;
  margin: 30rpx 0 0 0;
}
.root{
  display: flex;
  flex-direction: column;
  margin: 15rpx 0 15rpx 0;
  padding: 0 40rpx 0 40rpx;
}
.item{
  padding: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid gainsboro;
}
.item:last-child{
  border-bottom:none;
}